{
  "version": "4.1.4",
  "author": "Sunny Holic",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "page_action": {
    "default_icon": {
      "19": "icons/icon19.png",
      "38": "icons/icon38.png"
    },
    "default_title": "클린디씨",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "cleandc.js"
      ],
      "matches": [
        "*://gall.dcinside.com/*/*",
        "*://job.dcinside.com/*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "description": "쾌적한 갤질을 위해",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "manifest_version": 2,
  "name": "클린디씨",
  "options_page": "options.html",
  "permissions": [
    "contextMenus",
    "storage",
    "webRequest",
    "*://*.dcinside.com/*",
    "*://*.dcinside.co.kr/*",
    "tabs"
  ],
  "web_accessible_resources": [
    "helper.js",
    "assets/**",
    "*.png",
    "*.woff2"
  ],
  "short_name": "CleanDC"
}